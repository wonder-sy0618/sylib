<!--top顶部文件-->
<div class="panel top" >
    <!--logo区域-->
    <div class="topdb">
       <div class="topdbn">
            <table width="100%" border="0" cellspacing="0" cellpadding="0" class="tabb_top">
              <tr>
                <td class="logo"><img src="{{path_logo }}" /></td>
                <td align="right" valign="bottom">
					<p style="padding-bottom:2px; padding-right:10px;">欢迎您！<span class="red">admin</span></p>
				</td>
                <td width="140" align="center" style="padding-right:10px;">
					<!-- 导航菜单 -->
					{{each data_menus.navbarmenu as m index_m}}
                    <div class="margnl fr">
                        <a href="{{m.linkurl }}">
							<img src="{{path_resbase }}{{m.icon }}"  height="28" class="tuichu" />
                        <p>{{m.name }}</p>
                        </a>
                    </div>
					{{/each}}   <!-- /导航菜单 -->
                </td>
              </tr>
            </table>
       </div>
    </div>
    
    <!--顶部导航区-->
    <div class="daohang">
        <div class="daohangn">
            <div class="dhl fl">
                <ul>
					{{each data_menus.menus as menu1 index_menu1}}
                    <li  class="" index_menu1="{{index_menu1}}" >
                        <a href="{{menu1.linkurl }}" clientevent="{{menu1.clientevent }}" class="menuitem menu1" >
							<table border="0" cellspacing="0" cellpadding="0" class="dhtab">
                              <tr>
                                <td class="tdlf">&nbsp;</td>
                                <td class="tdmid">{{menu1.name }}</td>
                                <td class="tdrg">&nbsp;</td>
                              </tr>
                            </table>
                         </a>

                    </li>
					{{/each}}
                </ul>
                
            </div>
            <div class="fr">
                <div class="search">
                    <input name="" type="text" class="fl input_ss" />
                    <div class="fr">
                        <a href="#" class="ss"></a>
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    
    
    
</div>
<!--top顶部文件结束-->


<!--左边菜单开始-->
<div class="panel left">
	
	{{each data_menus.menus as menu1 index_menu1}}
    <div class="lfcon menu1group menu1group_{{index_menu1 }}" style="display:none;" index_menu1="{{index_menu1}}" >
        <div class="left_menu">
            <dl>
				{{each menu1.menus as menu2 index_menu2}}
                <dd class="ddhover {{if pageurl==menu2.linkurl}}lihover menuaction{{/if}}">
                    <a class="h1a menuitem menu2 {{menu2.class }}" 
					{{if !menu2.menus || menu2.menus.length <= 0}}href="{{menu2.linkurl }}"{{/if}} >
						<h1 {{if !menu2.menus || menu2.menus.length <= 0}}style="background-image: url();"{{/if}} 
						>{{menu2.name }}</h1>
					</a>
					{{if menu2.menus && menu2.menus.length > 0}}
                    <ul>
						{{each menu2.menus as menu3 index_menu3}}
                        <li class="{{if pageurl==menu3.linkurl}}lihover menuaction{{/if}}" >
							<a href="{{menu3.linkurl }}" clientevent="{{menu3.clientevent }}" class="menuitem menu3"  >
								<p class="lin" >{{menu3.name }}</p>
							</a>
						</li>
						{{/each}}
                    </ul>
                    {{/if}}
                </dd>
				{{/each}}
            </dl>
        </div>
        <div class="clear"></div>
    </div>    
	{{/each}}
        
</div>

<!--左边菜单结束-->


