    <div class="mainnr">
        <div class="d_position"><img src="{{path_resbase }}images/wizhi.png"/>当前位置：<span class="frcenter_local" ><span class="yellow">简历管理</span> / <span class="yellow">简历管理</span></span></div>
        <div class="mainnrbj main">
			
			
			
        </div>
    </div>
</div>
<!--右边主页面结束-->
